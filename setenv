#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
    if (argc != 3) {
        fprintf(stderr, "Usage: %s VARIABLE VALUE\n", argv[0]);
        exit(1);
    }

    if (setenv(argv[1], argv[2], 1) == -1) {
        perror("setenv failed");
        exit(1);
    }

    return 0;
}

